{
  "name": "@darkgaming/rescript-terrariapacket",
  "namespace": "TerrariaPacket",
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "tests",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true,
    "suffix": ".bs.js"
  },
  "bs-dependencies": [
    "@popstarfreas/packetfactory",
    "@popstarfreas/rescript-nodejs"
  ],
  "bs-dev-dependencies": [
    "@dusty-phillips/rescript-zora"
  ],
  "gentypeconfig": {
    "generatedFileExtension": ".gen.ts",
    "module": "commonjs",
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "warnings": {
    "number": ""
  }
}
