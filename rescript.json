{
  "name": "@darkgaming/rescript-terrariapacket",
  "namespace": "TerrariaPacket",
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "tests",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true,
    "suffix": ".bs.js"
  },
  "bs-dependencies": [
    "@popstarfreas/packetfactory",
    "@popstarfreas/rescript-nodejs",
    "@rescript/core"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ],
  "bs-dev-dependencies": [
    "@dusty-phillips/rescript-zora"
  ],
  "gentypeconfig": {
    "generatedFileExtension": ".gen.ts",
    "module": "commonjs",
    "shims": {
      "NodeJs": "NodeJs",
      "PacketFactory": "PacketFactory"
    },
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "warnings": {
    "number": "",
    "error": "+a"
  }
}
